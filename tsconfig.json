{
    "compilerOptions": {
        "target": "es5",
            "lib": [
                "dom",
                "dom.iterable",
                "esnext"
        ],
        "outDir": "public/build/",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "isolatedModules": true,
        "jsx": "react"
},
    "include": [
    "assets"
]
}